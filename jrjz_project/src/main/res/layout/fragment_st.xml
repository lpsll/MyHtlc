<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/ds_40"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/st_tv01"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/ds_120"
            android:layout_weight="1"
            android:layout_marginLeft="@dimen/ds_40"
            android:text="12:00 -- 14:00"
            android:textSize="@dimen/fs_45"
            android:padding="@dimen/ds_15"
            android:gravity="center"
            android:clickable="true"
            android:textColor="@color/navi"
            android:background="@drawable/btn_blue"/>

        <TextView
            android:id="@+id/st_tv02"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/ds_120"
            android:layout_weight="1"
            android:gravity="center"
            android:layout_marginLeft="@dimen/ds_40"
            android:layout_marginRight="@dimen/ds_40"
            android:padding="@dimen/ds_15"
            android:text="12:00 -- 14:00"
            android:textSize="@dimen/fs_45"
            android:clickable="true"
            android:textColor="@color/color_cc"
            android:background="@drawable/btn_gray"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/ds_40"
        android:orientation="horizontal">
